    "/blocks/{id}": {
      "get": {
        "summary": "GetBlockByID gets a full block by ID.",
        "operationId": "AccessAPI_GetBlockByID",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/flowaccessBlockResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "type": "string",
            "format": "byte"
          },
          {
            "name": "fullBlockResponse",
            "in": "query",
            "required": false,
            "type": "boolean"
          }
        ],
        "tags": [
          "AccessAPI"
        ]
      }
    },
    "/ping": {
      "get": {
        "summary": "Ping is used to check if the access node is alive and healthy.",
        "operationId": "AccessAPI_Ping",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
