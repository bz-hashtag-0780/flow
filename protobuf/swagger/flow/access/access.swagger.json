            }
          }
        },
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/flowaccessExecuteScriptAtLatestBlockRequest"
            }
          }
        ],
        "tags": [
          "AccessAPI"
        ]
      }
    },
    "/transaction_results/{id}": {
      "get": {
